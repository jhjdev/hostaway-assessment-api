{
  "name": "hostaway-assessment-api",
  "version": "1.0.0",
  "description": "An API service for the Hostaway weather app.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "start:debug": "echo 'Current dir:' && pwd && echo 'Files:' && ls -la && echo 'Dist contents:' && ls -la dist/ && node ./dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "seed": "tsx src/scripts/seed.ts"
  },
  "engines": {
    "node": "22.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhjdev/hostaway-assessment-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jhjdev/hostaway-assessment-api/issues"
  },
  "homepage": "https://github.com/jhjdev/hostaway-assessment-api#readme",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "fastify": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "nodemailer": "^7.0.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.8",
    "@types/nodemailer": "^6.4.17",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
